<p id="notice"><%= notice %></p>

<h1>Listing Leads</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Phone</th>
      <th>Project</th>
      <th>Project name</th>
      <th>F 1</th>
      <th>G 2</th>
      <th>G 3</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @leads.each do |lead| %>
      <tr>
        <td><%= lead.name %></td>
        <td><%= lead.phone %></td>
        <td><%= lead.project_id %></td>
        <td><%= lead.project_name %></td>
        <td><%= lead.f_1 %></td>
        <td><%= lead.g_2 %></td>
        <td><%= lead.g_3 %></td>
        <td><%= link_to 'Show', lead %></td>
        <td><%= link_to 'Edit', edit_lead_path(lead) %></td>
        <td><%= link_to 'Destroy', lead, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Lead', new_lead_path %>
